@base <http://localhost:8080/workspaces/sub> .
@prefix hctl: <https://www.w3.org/2019/wot/hypermedia#> .
@prefix htv: <http://www.w3.org/2011/http#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix urn: <http://example.org/urn#> .
@prefix ex: <http://example.org/> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .
@prefix hmas: <https://purl.org/hmas/> .

<> a hmas:ResourceProfile;
  hmas:exposesSignifier [ a hmas:Signifier;
      hmas:signifies [ a sh:NodeShape;
          sh:class hmas:ActionExecution;
          sh:property [
              sh:path prov:used;
              sh:minCount "1"^^xs:int;
              sh:maxCount "1"^^xs:int;
              sh:hasValue <#createSubWorkspace>
            ]
        ]
    ], [ a hmas:Signifier;
      hmas:signifies [ a sh:NodeShape;
          sh:class hmas:ActionExecution;
          sh:property [
              sh:path prov:used;
              sh:minCount "1"^^xs:int;
              sh:maxCount "1"^^xs:int;
              sh:hasValue <#makeArtifact>
            ], [
              sh:path hmas:hasInput
            ]
        ]
    ], [ a hmas:Signifier;
      hmas:signifies [ a sh:NodeShape;
          sh:class hmas:ActionExecution;
          sh:property [
              sh:path prov:used;
              sh:minCount "1"^^xs:int;
              sh:maxCount "1"^^xs:int;
              sh:hasValue <#leaveWorkspace>
            ]
        ]
    ], [ a hmas:Signifier;
      hmas:signifies [ a sh:NodeShape;
          sh:class hmas:ActionExecution;
          sh:property [
              sh:path prov:used;
              sh:minCount "1"^^xs:int;
              sh:maxCount "1"^^xs:int;
              sh:hasValue <#focusWorkspace>
            ], [
              sh:path hmas:hasInput
            ]
        ]
    ], [ a hmas:Signifier;
      hmas:signifies [ a sh:NodeShape;
          sh:class hmas:ActionExecution;
          sh:property [
              sh:path prov:used;
              sh:minCount "1"^^xs:int;
              sh:maxCount "1"^^xs:int;
              sh:hasValue <#joinWorkspace>
            ]
        ]
    ];
  hmas:isProfileOf <#workspace>;
  hmas:isContainedIn <test>;
  hmas:contains <sub/artifacts/c0>, <sub/agents/test_agent> .

<#createSubWorkspace> a hctl:Form;
  hctl:hasTarget <>;
  htv:methodName "POST";
  hctl:forContentType "application/json" .

<#makeArtifact> a hctl:Form;
  hctl:hasTarget <sub/artifacts/>;
  htv:methodName "POST";
  hctl:forContentType "application/json" .

<#leaveWorkspace> a hctl:Form;
  hctl:hasTarget <sub/leave>;
  htv:methodName "POST";
  hctl:forContentType "application/json" .

<#focusWorkspace> a hctl:Form;
  hctl:hasTarget <sub/focus>;
  htv:methodName "POST";
  hctl:forContentType "application/json" .

<#joinWorkspace> a hctl:Form;
  hctl:hasTarget <sub/join>;
  htv:methodName "POST";
  hctl:forContentType "application/json" .

<#workspace> a hmas:Workspace .

<test> a hmas:Workspace .

<sub/artifacts/c0> a hmas:Artifact .

<sub/agents/test_agent> a hmas:Artifact .
