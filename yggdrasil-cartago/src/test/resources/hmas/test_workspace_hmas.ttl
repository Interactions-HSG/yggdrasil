          sh:path prov:used;
          sh:minCount "1"^^xs:int;
          sh:maxCount "1"^^xs:int;
          sh:hasValue <http://localhost:8080/workspaces/test/#updateCurrentWorkspaceForm>
        ]
    ] .

<http://localhost:8080/workspaces/test/#updateCurrentWorkspaceForm> a hctl:Form;
